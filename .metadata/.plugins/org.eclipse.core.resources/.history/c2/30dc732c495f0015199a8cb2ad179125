	<script type="text/javascript">
	document.getElementById("todayDate").innerHTML = ("8/18/2015 0:01");
	function generateDate(vIn){
		var a = vIn.split("/");
		var month = parseInt(a[0]);
		var day = parseInt(a[1]);
		var b = a[2].split(" ");
		var year = parseInt(b[0]);
		var c = b[1].split(":");
		var hour = parseInt(c[0]);
		var minute = parseInt(c[1]);
		return new Date(year, month, day, hour, minute)
	}
	alert(Date.parse("8/18/2015 0:01"));
	</script>
